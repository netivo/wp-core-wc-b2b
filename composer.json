{
  "name": "netivo/wc-b2b",
  "description": "Netivo Module for B2B panel.",
  "minimum-stability": "stable",
  "license": "proprietary",
  "version": "1.0",
  "authors": [
    {
      "name": "netivo",
      "email": "biuro@netivo.pl"
    },
    {
      "name": "Michał Świątek",
      "email": "michal.swiatek@netivo.pl"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:netivo/wp-core.git"
    }
  ],
  "require": {
    "php": ">=8.2",
    "netivo/wp-core": "^1.1",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5 || ^11.0",
    "brain/monkey": "^2.6"
  },
  "autoload": {
    "psr-4": {
      "Netivo\\Module\\WooCommerce\\B2B\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Netivo\\Module\\WooCommerce\\B2B\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": [
      "@php -d zend.enable_gc=0 vendor/bin/phpunit"
    ],
    "test-debug": [
      "@php -d zend.enable_gc=0 vendor/bin/phpunit --debug"
    ]
  }
}